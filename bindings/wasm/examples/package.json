{
  "name": "manifold-examples",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:vite && npm run build:transpile",
    "build:vite": "tsc --noEmit && vite build",
    "build:transpile": "npx tsc -p lib --outDir built/lib",
