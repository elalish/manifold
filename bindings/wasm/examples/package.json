    "monaco-editor": "^0.54.0",
    "monaco-editor-auto-typings": "^0.4.6",
    "simple-dropzone": "0.8.3",
    "three": "0.182.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
