    "@gltf-transform/functions": "^3.8.0",
    "@jscadui/3mf-export": "^0.5.0",
    "@types/three": "^0.164.0",
    "commander": "^13.1.0",
    "fflate": "^0.8.0",
    "gl-matrix": "^3.4.3",
    "simple-dropzone": "0.8.3",
    "three": "0.164.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/ui": "^1.6.0",
    "typescript": "^5.7.0",
    "vite": "^4.5.0",
    "vitest": "^1.6.0"
  }
