{
  "name": "manifold-3d",
  "version": "3.3.2",
  "description": "Geometry library for topological robustness",
  "main": "manifold.js",
  "bin": {
    "manifold-cad": "./bin/manifold-cad"
  },
  "scripts": {
    "build": "tsc && node ./esbuild.mjs && npm run build:api-extractor",
    "build:api-extractor": "api-extractor run -c api-extractor-manifoldCAD.json && api-extractor run -c api-extractor-manifoldCADGlobals.json",
    "clean": "tsc --build --clean",
    "format": "cd ../.. && ./scripts/format.sh",
    "docs": "npx typedoc",
    "test": "vitest run",
    "test:ui": "vitest --coverage --ui",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "npm run build"
  },
  "files": [
    "manifold.js",
    "manifold.wasm",
    "manifold.d.ts",
    "manifold-encapsulated-types.d.ts",
    "manifold-global-types.d.ts",
    "bin/manifold-cad",
    "lib/*.d.ts",
    "lib/*.js",
    "types/manifoldCAD.d.ts",
    "types/manifoldCADGlobals.d.ts",
    "dist/manifoldCAD.d.ts",
    "dist/manifoldCADGlobals.d.ts",
    "dist/worker.bundled.js"
  ],
  "exports": {
    "./manifoldCAD": {
      "import": "./lib/manifoldCAD.js",
      "types": "./dist/manifoldCAD.d.ts"
    },
    "./manifoldCAD.d.ts": "./dist/manifoldCAD.d.ts",
    "./lib/worker.bundled.js": {
      "import": "./dist/worker.bundled.js",
      "types": "./lib/worker.d.ts"
    },
    "./lib/*": "./lib/*",
    "./manifold.wasm": "./manifold.wasm",
    ".": "./manifold.js"
  },
  "typings": "manifold.d.ts",
  "types": "manifold.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elalish/manifold.git"
  },
  "keywords": [
    "3D",
    "mesh",
    "manifold",
    "geometry",
    "solid",
    "CAD",
    "triangle",
    "SDF"
  ],
  "contributors": [
    "Emmett Lalish <elalish>",
    "Chun Kit LAM <pca006132>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/elalish/manifold/issues"
  },
  "homepage": "https://github.com/elalish/manifold#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.53.3",
    "@types/convert-source-map": "^2.0.3",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.25.11",
    "glob": "^11.0.3",
    "stacktrace-parser": "^0.1.11",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@gltf-transform/core": "^4.2.0",
    "@gltf-transform/extensions": "^4.2.0",
    "@gltf-transform/functions": "^4.2.0",
    "@jridgewell/trace-mapping": "^0.3.31",
    "@jscadui/3mf-export": "^0.5.0",
    "commander": "^13.1.0",
    "convert-source-map": "^2.0.0",
    "esbuild-plugin-text-replace": "^1.3.0",
    "esbuild-wasm": "^0.25.11",
    "fflate": "^0.8.0"
  }
}
