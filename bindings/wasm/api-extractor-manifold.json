{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./manifold-root.d.ts",
  "compiler": {
    "overrideTsconfig": {
      "include": [
        "./manifold-global-types.d.ts",
        "./manifold-encapsulated-types.d.ts",
        "./manifold-root.d.ts"
      ]
    }
  },
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "./manifold.d.ts"
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-missing-release-tag": {
         "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "tsdoc-malformed-html-name": {
        "logLevel": "none"
      },
      "tsdoc-param-tag-missing-hyphen": {
        "logLevel": "none"
      },
      "tsdoc-escape-right-brace": {
        "logLevel": "none"
      },
      "tsdoc-escape-greater-than": {
        "logLevel": "none"
      },
      "tsdoc-malformed-inline-tag": {
        "logLevel": "none"
      },
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      },
      "tsdoc-reference-missing-hash": {
        "logLevel": "none"
      }
    }
  }
}
