{
  "$schema": "https://typedoc.org/schema.json",
  "favicon": "https://manifoldcad.org/samples/models/favicon.png",
  "navigationLinks": {
    "ManifoldCAD.org": "https://manifoldCAD.org",
    "ManifoldCAD User Guide": "https://manifoldcad.org/docs/jsuser/",
    "JS/TS/WASM API": "https://manifoldcad.org/docs/jsapi/",
    "C++ API": "https://manifoldcad.org/docs/html/",
    "Algorithm Description": "https://github.com/elalish/manifold/wiki/Manifold-Library",
    "Blog Posts": "https://elalish.blogspot.com/search/label/Manifold",
    "GitHub": "https://github.com/elalish/manifold"
  },
  "externalSymbolLinkMappings": {
    "@gltf-transform/core": {
      "Accessor": "https://gltf-transform.dev/modules/core/classes/Accessor",
      "Document": "https://gltf-transform.dev/modules/core/classes/Document",
      "Extension": "https://gltf-transform.dev/modules/core/classes/Extension",
      "ExtensionProperty": "https://gltf-transform.dev/modules/core/classes/ExtensionProperty",
      "Node": "https://gltf-transform.dev/modules/core/classes/Node",
      "*": "https://gltf-transform.dev/"
    }
  },
  "searchInComments": true,
  "searchInDocuments": true,
  "useFirstParagraphOfCommentAsSummary": true,
  "excludeExternals": true,
  "defaultCategory": "None",
  "categorizeByGroup": false,
  "groupReferencesByType": false,
  "navigation": {
    "includeFolders": false,
    "includeCategories": false,
    "includeGroups": false
  },
  "exclude": [
    "**/*.test.*",
    "./lib/*.d.ts"
  ],
  "groupOrder": [
    "Documents", 
    "Interfaces", "Classes",
    "Basics",
    "Constructors", "Properties", "Accessors",
    "Input & Output", "Polygons", 
    "Material", "Scene Graph", "Animation", "Level of Detail",
    "Information", "Measurement", "Mesh ID", "Transformations",
    "Boolean", "Properties", "Smoothing", "Convex Hull",
    "Functions", "Methods", "*"
  ],
  "kindSortOrder": [
    "Reference", "Project", "Module", "Namespace",
    "Interface", "Enum", "EnumMember",
    "Class", "Constructor",
    "Property", "Variable", "Accessor",
    "Function", "Method", "Parameter",
    "CallSignature", "ConstructorSignature", "IndexSignature", "GetSignature", "SetSignature",
    "TypeParameter", "TypeLiteral", "TypeAlias"
  ],
  "sort": [
    "documents-first",
    "kind",
    "instance-first",
    "alphabetical-ignoring-documents"
  ],
  "notRenderedTags": [
    "@showCategories",
    "@showGroups",
    "@hideCategories",
    "@hideGroups",
    "@disableGroups",
    "@expand",
    "@preventExpand",
    "@expandType",
    "@summary",
    "@group",
    "@groupDescription",
    "@category",
    "@categoryDescription",
    "@sortStrategy"
  ]
}