{
  "$schema": "https://typedoc.org/schema.json",
  "extends": ["./typedoc.common.json"],
  "name": "Manifold WASM Developer Guide",
  "customCss": "./documents/developer-guide.css",
  "out": "docs/jsapi",
  "entryPoints": [
    "./manifold.d.ts",
    "./lib/*.ts"
  ],
  "exclude": [
    "./lib/scene-builder.ts",
    "./**/*.test.ts",
    "./lib/*.d.ts"
  ],
  "sortEntryPoints": false,
  "projectDocuments": [
    "./documents/embedding-manifoldcad.md",
    "./documents/bindings.md",
    "./documents/bindings-examples.md",
    "./documents/contributing.md"
  ],
  "navigation": {
    "includeGroups": true,
  },
  "groupReferencesByType": true,
  "defaultCategory": "none"
}
