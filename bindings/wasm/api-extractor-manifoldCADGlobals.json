{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./types/manifoldCADGlobals.d.ts",
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "./dist/manifoldCADGlobals.d.ts"
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-missing-release-tag": {
         "logLevel": "none"
      },
      "ae-unresolved-link": {
        "logLevel": "none"
      },
      "ae-incompatible-release-tags": {
        "logLevel": "none"
      },
      "ae-internal-missing-underscore": {
        "logLevel": "none"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "tsdoc-malformed-html-name": {
        "logLevel": "none"
      },
      "tsdoc-param-tag-missing-hyphen": {
        "logLevel": "none"
      },
      "tsdoc-escape-right-brace": {
        "logLevel": "none"
      },
      "tsdoc-escape-greater-than": {
        "logLevel": "none"
      },
      "tsdoc-malformed-inline-tag": {
        "logLevel": "none"
      },
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      },
      "tsdoc-reference-missing-hash": {
        "logLevel": "none"
      }
    }
  }
}
